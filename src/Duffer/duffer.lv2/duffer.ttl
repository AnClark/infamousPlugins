@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .

<http://infamousplugins.sourceforge.net/plugs.html#duffer>
        a lv2:Plugin, lv2:ModulatorPlugin ;
        doap:name "the infamous duffer" ;
        doap:maintainer [
                foaf:name "Spencer Jackson" ;
                foaf:homepage <http://infamousplugins.sourceforge.net> ;
                foaf:mbox <ssjackson71@gmail.com> ;
        ] ;
        lv2:requiredFeature <http://lv2plug.in/ns/ext/urid#map> ;
        lv2:optionalFeature lv2:hardRTCapable ,
            <http://lv2plug.in/ns/ext/bufsz#maxBlockLength>;


        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT" ;
                lv2:name "Audio In" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "OUTPUT" ;
                lv2:name "Audio Out" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 2 ;
                lv2:symbol "DELTA" ;
                lv2:name "Damping" ;
                lv2:default 0.5 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 3 ;
                lv2:symbol "ALPHA" ;
                lv2:name "Spring Nonlinearity" ;
                lv2:default 0.50 ;
                lv2:minimum -1.0 ;
                lv2:maximum 1.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "BETA" ;
                lv2:name "Spring Stiffness" ;
                lv2:default 0.5 ;
                lv2:minimum -1.0 ;
                lv2:maximum 1.0 ; 
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "GAMMA" ;
                lv2:name "Input Gain" ;
                lv2:default 1.0 ;
                lv2:minimum 0.00 ;
                lv2:maximum 2.0 ; 
        ] ;
