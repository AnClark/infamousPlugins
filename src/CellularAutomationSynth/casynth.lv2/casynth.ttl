@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

@prefix lv2:     <http://lv2plug.in/ns/lv2core#> .
@prefix lv2ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix lv2atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix lv2midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix lv2time: <http://lv2plug.in/ns/ext/time#> .

<http://sourceforge.net/projects/infamousplugins:casynth>
        a lv2:Plugin, lv2:InstrumentPlugin ;
        doap:name "cellular automation synth" ;
        doap:maintainer [
                foaf:name "Spencer Jackson" ;
                foaf:homepage <http://sourceforge.net/projects/infamousplugins/> ;
                foaf:mbox <ssjackson71@gmail.com> ;
        ] ;
        lv2:requiredFeature <http://lv2plug.in/ns/ext/urid#map> ;
        lv2:optionalFeature lv2:hardRTCapable ;

        lv2:port [
                a lv2:InputPort, lv2atom:AtomPort ;
                lv2atom:bufferType lv2atom:Sequence ;
                lv2atom:supports lv2midi:MidiEvent ;
                lv2:index 0 ;
                lv2:symbol "MIDI_IN";
                lv2:name "MIDI In";
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "OUTPUT" ;
                lv2:name "Audio Out" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index  2 ;
                lv2:symbol "CHANNEL" ;
                lv2:name "MIDI Channel" ;
                lv2:portProperty lv2:integer ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 16.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 3 ;
                lv2:symbol "MASTER_GAIN" ;
                lv2:name "Master Gain" ;
                lv2:default 1.0 ;
                lv2:minimum 0.001 ;
                lv2:maximum 2.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "RULE" ;
                lv2:name "Cell Automation Rule" ;
                lv2:portProperty lv2:integer ;
                lv2:default 30.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 255.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "CELL_LIFE" ;
                lv2:name "Cell Lifetime" ;
                lv2:portProperty lv2:logarithmic ;
                lv2:default 0.25 ;
                lv2:minimum 0.001 ;
                lv2:maximum 10.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "INIT_CELLS" ;
                lv2:name "Initial Condition" ;
                lv2:portProperty lv2:integer ;
                lv2:default 1.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 65535.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "NHARMONICS" ;
                lv2:name "Harmonics" ;
                lv2:portProperty lv2:integer ;
                lv2:default 12.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 16.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "HARM_MODE" ;
                lv2:name "Harmonic Gain Mode" ;
                lv2:portProperty lv2:integer ;
                lv2:scalePoint [ rdfs:label "Sinc"; rdf:value 0 ] ;
                lv2:scalePoint [ rdfs:label "Saw"; rdf:value 1 ] ;
                lv2:scalePoint [ rdfs:label "Square"; rdf:value 2 ] ;
                lv2:scalePoint [ rdfs:label "Triangle"; rdf:value 3 ] ;
                lv2:default 1.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 3.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "WAVE" ;
                lv2:name "Waveform" ;
                lv2:portProperty lv2:integer ;
                lv2:scalePoint [ rdfs:label "Sine"; rdf:value 0 ] ;
                lv2:scalePoint [ rdfs:label "Saw"; rdf:value 1 ] ;
                lv2:scalePoint [ rdfs:label "Square"; rdf:value 2 ] ;
                lv2:scalePoint [ rdfs:label "Triangle"; rdf:value 3 ] ;
                lv2:scalePoint [ rdfs:label "White Noise"; rdf:value 4 ] ;
                lv2:scalePoint [ rdfs:label "Random"; rdf:value 5 ] ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 5.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "ENV_A" ;
                lv2:name "Attack" ;
                lv2:portProperty lv2:logarithmic ;
                lv2:default 0.10 ;
                lv2:minimum 0.001 ;
                lv2:maximum 15.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "ENV_D" ;
                lv2:name "Decay" ;
                lv2:portProperty lv2:logarithmic ;
                lv2:default 0.10 ;
                lv2:minimum 0.001 ;
                lv2:maximum 15.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 12 ;
                lv2:symbol "ENV_B" ;
                lv2:name "Break Point" ;
                lv2:default 0.5 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 13 ;
                lv2:symbol "ENV_SWL" ;
                lv2:name "Swell" ;
                lv2:portProperty lv2:logarithmic ;
                lv2:default 3.0 ;
                lv2:minimum 0.001 ;
                lv2:maximum 15.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 14 ;
                lv2:symbol "ENV_SUS" ;
                lv2:name "Sustain" ;
                lv2:default 0.75 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 15 ;
                lv2:symbol "ENV_R" ;
                lv2:name "Release" ;
                lv2:portProperty lv2:logarithmic ;
                lv2:default 0.1 ;
                lv2:minimum 0.001 ;
                lv2:maximum 15.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 16 ;
                lv2:symbol "AMOD_WAV" ;
                lv2:name "AM Waveform" ;
                lv2:portProperty lv2:integer ;
                lv2:scalePoint [ rdfs:label "Sine"; rdf:value 0 ] ;
                lv2:scalePoint [ rdfs:label "Saw"; rdf:value 1 ] ;
                lv2:scalePoint [ rdfs:label "Square"; rdf:value 2 ] ;
                lv2:scalePoint [ rdfs:label "Triangle"; rdf:value 3 ] ;
                lv2:scalePoint [ rdfs:label "White Noise"; rdf:value 4 ] ;
                lv2:scalePoint [ rdfs:label "Random"; rdf:value 5 ] ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 5.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 17 ;
                lv2:symbol "AMOD_FREQ" ;
                lv2:name "AM Frequency" ;
                lv2:portProperty lv2:logarithmic ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 110.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 18 ;
                lv2:symbol "AMOD_GAIN" ;
                lv2:name "AM Gain" ;
                lv2:default 0.10 ;
                lv2:minimum -1.00 ;
                lv2:maximum 1.00 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 19 ;
                lv2:symbol "FMOD_WAV" ;
                lv2:name "FM Waveform" ;
                lv2:portProperty lv2:integer ;
                lv2:scalePoint [ rdfs:label "Sine"; rdf:value 0 ] ;
                lv2:scalePoint [ rdfs:label "Saw"; rdf:value 1 ] ;
                lv2:scalePoint [ rdfs:label "Square"; rdf:value 2 ] ;
                lv2:scalePoint [ rdfs:label "Triangle"; rdf:value 3 ] ;
                lv2:scalePoint [ rdfs:label "White Noise"; rdf:value 4 ] ;
                lv2:scalePoint [ rdfs:label "Random"; rdf:value 5 ] ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 5.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 20 ;
                lv2:symbol "FMOD_FREQ" ;
                lv2:name "FM Frequency" ;
                lv2:portProperty lv2:logarithmic ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 110.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 21 ;
                lv2:symbol "FMOD_GAIN" ;
                lv2:name "FM Gain" ;
                lv2:default 0.10 ;
                lv2:minimum -2.00 ;
                lv2:maximum 2.00 ;
        ].
