@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props> .

@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .

<http://infamousplugins.sourceforge.net/plugs.html#cheapdist>
        a lv2:Plugin, lv2:DistortionPlugin ;
        doap:name "the infamous cheap distortion" ;
        doap:maintainer [
                foaf:name "Spencer Jackson" ;
                foaf:homepage <http://infamousplugins.sourceforge.net> ;
                foaf:mbox <ssjackson71@gmail.com> ;
        ] ;
        lv2:optionalFeature lv2:hardRTCapable ;
        ui:ui <http://infamousplugins.sourceforge.net/plugs.html#cheapdist_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT" ;
                lv2:name "Audio In" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "OUTPUT" ;
                lv2:name "Audio Out" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 2 ;
                lv2:symbol "AGGRESSION" ;
                lv2:name "aggression" ;
                lv2:portProperty lv2:integer ;
        ] ;
          .
