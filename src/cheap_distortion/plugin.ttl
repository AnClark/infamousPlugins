@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .

<http://sourceforge.net/projects/infamousplugins:cheapdist>
 a lv2:Plugin, lv2:WaveshaperPlugin ;
 doap:name "cheap distortion" ;
 doap:maintainer [ foaf:name "Spencer Jackson" ] ;

 lv2:optionalFeature lv2:hardRTCapable ;

 lv2:requiredFeature <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ,
  <http://lv2plug.in/ns/ext/urid#map> ;

 lv2:port [
  a lv2:InputPort, lv2:AudioPort ;
  lv2:index 0 ;
  lv2:name "Audio In 1" ;
  lv2:symbol "AUDIO_IN_1" ;
 ] ,

 [
  a lv2:OutputPort, lv2:AudioPort ;
  lv2:index 1 ;
  lv2:name "Audio Out 1" ;
  lv2:symbol "AUDIO_OUT_1" ;
 ] ,

 [
  a lv2:InputPort, lv2:ControlPort ;
  lv2:index 2 ;
  lv2:name "Aggresion" ;
  lv2:symbol "AGGRESSION" ;
  lv2:default 1 ;
  lv2:minimum 1 ;
  lv2:maximum 3 ;
  lv2:portProperty lv2:enumeration ;
  lv2:scalePoint [ rdfs:label "1"; rdf:value 1 ],
   [ rdfs:label "2"; rdf:value 2 ],
   [ rdfs:label "3"; rdf:value 3 ];
 ].