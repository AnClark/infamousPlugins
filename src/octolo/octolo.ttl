@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix lv2atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix lv2time: <http://lv2plug.in/ns/ext/time#> .
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .

@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .

<http://ssj71.github.io/infamousPlugins/plugs.html#octolo>
        a lv2:Plugin, lv2:ModulatorPlugin ;
        doap:name "the infamous octolo" ;
        doap:license <http://opensource.org/licenses/GPL-2.0> ;
        doap:description "an octaved tremolo";
        doap:maintainer [
                foaf:name "Spencer Jackson" ;
                foaf:homepage <http://ssj71.github.io/infamousPlugins> ;
                foaf:mbox <mailto:ssjackson71@gmail.com> ;
        ] ;
        lv2:minorVersion 0 ;
        lv2:microVersion 1 ;
        rdfs:comment "a combination octaver and tremolo with mix, phase control, and sequencing";
        lv2:optionalFeature lv2:hardRTCapable ;

        lv2:port [
                a lv2:InputPort, lv2atom:AtomPort ;
                lv2atom:bufferType lv2atom:Sequence ;
                lv2atom:supports lv2time:Position ;
                lv2:index 0 ;
                lv2:symbol "ATOM_IN";
                lv2:name "Atom In";
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "INPUT" ;
                lv2:name "Audio In" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 2 ;
                lv2:symbol "OUTPUT" ;
                lv2:name "Audio Out" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 3 ;
                lv2:symbol "ENABLE" ;
                lv2:name "Enable" ;
                lv2:default 1 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
                lv2:designation lv2:enabled ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "LENGTH" ;
                lv2:name "Length" ;
                lv2:default 1.0 ;
                lv2:minimum 0.01 ;
                lv2:maximum 8.0 ;
                units:unit units:beat ;
                #lv2:portProperty pprops:logarithmic ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "DRY" ;
                lv2:name "Dry" ;
                lv2:default 0.5 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
                units:unit units:coef ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "WET" ;
                lv2:name "Wet" ;
                lv2:default 0.5 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
                units:unit units:coef ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "OCTDOWN" ;
                lv2:name "Octave Down" ;
                lv2:default 0.5 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
                units:unit units:coef ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "OCTUP" ;
                lv2:name "Octave Up" ;
                lv2:default 0.5 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
                units:unit units:coef ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "SLOPE" ;
                lv2:name "Slope" ;
                lv2:default 1.0 ;
                lv2:minimum 0.001 ;
                lv2:maximum 1.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "SEQUENCE" ;
                lv2:name "Sequence" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 5 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:enumeration ;
                lv2:scalePoint [ rdfs:label "Sync"; rdf:value 0 ] ;
                lv2:scalePoint [ rdfs:label "Alternate 1 Oct"; rdf:value 1 ] ;
                lv2:scalePoint [ rdfs:label "Alternate 2 Oct"; rdf:value 2 ] ;
                lv2:scalePoint [ rdfs:label "Step"; rdf:value 3 ] ;
                lv2:scalePoint [ rdfs:label "Cycle Up"; rdf:value 4 ] ;
                lv2:scalePoint [ rdfs:label "Cycle Down"; rdf:value 5 ] ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "OVERLAP" ;
                lv2:name "Overlap" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ] .

