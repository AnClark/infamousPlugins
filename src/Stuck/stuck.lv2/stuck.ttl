@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .

<http://infamousplugins.sourceforge.net/plugs.html#stuck>
        a lv2:Plugin, lv2:DelayPlugin ;
        doap:name "the infamous stuck" ;
        doap:maintainer [
                foaf:name "Spencer Jackson" ;
                foaf:homepage <http://infamousplugins.sourceforge.net> ;
                foaf:mbox <ssjackson71@gmail.com> ;
        ] ;
        lv2:requiredFeature <http://lv2plug.in/ns/ext/urid#map> ;
        lv2:optionalFeature lv2:hardRTCapable ;


        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT" ;
                lv2:name "Audio In" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "OUTPUT" ;
                lv2:name "Audio Out" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 2 ;
                lv2:symbol "STICK_IT" ;
                lv2:name "Stick it!" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
		lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 3 ;
                lv2:symbol "DRONE_GAIN" ;
                lv2:name "Drone Gain" ;
                lv2:default 0.50 ;
                lv2:minimum 0.0 ;
                lv2:maximum 2.0 ;
		units:unit units:coeff ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "RELEASE" ;
                lv2:name "Release" ;
                lv2:default 0.5 ;
                lv2:minimum 0.01 ;
                lv2:maximum 3.0 ; 
		units:unit units:s ;
        ] ;
