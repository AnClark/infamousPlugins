@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

@prefix lv2:     <http://lv2plug.in/ns/lv2core#> .
@prefix lv2ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix pset: <http://lv2plug.in/ns/ext/presets#> .
@prefix lv2atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix lv2midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix lv2time: <http://lv2plug.in/ns/ext/time#> .
@prefix pprops:<http://lv2plug.in/ns/ext/port-props#> .
@prefix pg: <http://lv2plug.in/ns/ext/port-groups#> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .

<http://infamousplugins.sourceforge.net/plugs.html#hip2b>
        a lv2:Plugin, lv2:DistortionPlugin ;
        doap:name "Hip2B" ;
        doap:maintainer [
                foaf:name "Spencer Jackson" ;
                foaf:homepage <http://infamousplugins.sourceforge.net> ;
                foaf:mbox <ssjackson71@gmail.com> ;
        ] ;
        lv2:requiredFeature <http://lv2plug.in/ns/ext/urid#map> ;
        lv2:optionalFeature lv2:hardRTCapable ;


        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT" ;
                lv2:name "Audio In" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "OUTPUT" ;
                lv2:name "Audio Out" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:ControlPort ;
                lv2:index 2 ;
                lv2:symbol "LATENCY" ;
                lv2:name "Latency" ;
                lv2:portProperty lv2:reportsLatency, lv2:integer, pprops:notOnGUI;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 3 ;
                lv2:symbol "UP" ;
                lv2:name "Up" ;
                lv2:default 0.02 ;
                lv2:minimum -1.0 ;
                lv2:maximum 1.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "DOWN" ;
                lv2:name "Down" ;
                lv2:default 0.02 ;
                lv2:minimum -1.0 ;
                lv2:maximum 1.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "IN_GAIN" ;
                lv2:name "Input Gain" ;
                lv2:default 1.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 2.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "WETDRY" ;
                lv2:name "Wet/Dry Mix" ;
                lv2:default 1.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "OUT_GAIN" ;
                lv2:name "Output Gain" ;
                lv2:default 1.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 2.0 ;
        ] .
